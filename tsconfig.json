{
  "compilerOptions": {
    "baseUrl": ".",
    "inlineSourceMap": true,
    "inlineSources": true,
    "module": "commonjs",
    "target": "es6",
    "moduleResolution": "node",
    "jsx": "react",
    "esModuleInterop": true,
    "strict": true,
    "noImplicitAny": false,
    "allowJs": true,
    "resolveJsonModule": true,
    "lib": ["dom", "es6"],
    "types": ["node", "obsidian"],
    "outDir": "./"
  },
  "include": ["**/*.ts"],
  "exclude": ["node_modules"]
}


